{
  "name": "testeranto",
  "description": "teeny tiny tightly-typed typescript tests",
  "version": "0.40.0",
  "types": "./dist/index.d.mts",
  "main": "dist/common/index.js",
  "module": "dist/module/index.js",
  "exports": {
    ".": {
      "import": "./dist/module/index.js",
      "require": "./dist/common/index.js"
    },
    "./src/Features": {
      "import": "./dist/module/Features.js",
      "require": "./dist/common/Features.js"
    },
    "./src/Project": {
      "import": "./dist/module/Project.js",
      "require": "./dist/common/Project.js"
    },
    "./src/core-electron": {
      "import": "./dist/module/core-electron.js",
      "require": "./dist/common/core-electron.js"
    },
    "./src/core-node": {
      "import": "./dist/module/core-node.js",
      "require": "./dist/common/core-node.js"
    },
    "./src/core-puppeteer": {
      "import": "./dist/module/core-puppeteer.js",
      "require": "./dist/common/core-puppeteer.js"
    },
    "./src/Puppeteer": {
      "import": "./dist/module/Puppeteer.js",
      "require": "./dist/common/Puppeteer.js"
    },
    "./src/electron": {
      "import": "./dist/module/electron.js",
      "require": "./dist/common/electron.js"
    },
    "./src/core": {
      "import": "./dist/module/core.js",
      "require": "./dist/common/core.js"
    },
    "./src/preload": {
      "import": "./dist/module/preload.js",
      "require": "./dist/common/preload.js"
    }
  },
  "repository": "git@github.com:adamwong246/testeranto.git",
  "homepage": "https://github.com/adamwong246/testeranto",
  "author": "adam wong <adamwong246@gmail.com>",
  "license": "MIT",
  "keywords": [
    "typescript",
    "test",
    "cucumber",
    "ATDD"
  ],
  "bin": {
    "testerantoRun": "./bin/testerantoRun.sh",
    "testerantoWatch": "./bin/testerantoWatch.sh"
  },
  "scripts": {
    "build": "sh prebuild.sh; tsc -p tsconfig.module.json; tsc -p tsconfig.common.json; tsc -p tsconfig.types.json; sh postBuild.sh",
    "bundle:reporter": "esbuild ./src/Report.tsx --bundle --outfile=./dist/reporter.js",
    "simpleserver": "cd dist && python -m SimpleHTTPServer 8080",
    "clean:dist": "rm -rf ./dist/results && rm -rf ./dist/tests  && rm -f ./dist/reporter.css && rm -f ./dist/reporter.js && rm -f ./dist/*.jpg ",
    "clean:npm": "rm -rf node_modules && yarn install",
    "clean": "yarn clean:dist && yarn clean:npm",
    "tableflip": "yarn clean && yarn bundle:reporter"
  },
  "peerDependencies": {
    "electron": "^24.2.0",
    "esbuild": "0.17.5"
  },
  "devDependencies": {
    "esbuild": "0.17.5"
  },
  "dependencies": {
    "@swc/core": "^1.3.26",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.21",
    "@types/react-bootstrap": "^0.32.32",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-loader": "^8.2.5",
    "bootstrap": "^5.2.3",
    "electron": "^29.1.1",
    "esbuild-plugin-external-package": "^1.0.0",
    "esbuild-plugin-import-map": "^2.1.0",
    "eslint": "^8.29.0",
    "eslint-plugin-react": "^7.31.11",
    "fresh-require": "^1.0.3",
    "fs-extra": "^11.2.0",
    "fs.promises.exists": "^1.1.4",
    "graphology": "^0.25.1",
    "graphology-dag": "^0.2.0",
    "graphology-types": "^0.24.5",
    "ipc": "^0.0.1",
    "pm2": "^5.2.2",
    "puppeteer": "18.1.0",
    "react-bootstrap": "^2.7.0",
    "ts-node": "^10.9.1",
    "typescript": "4.8.2",
    "why-is-node-running": "^2.2.2",
    "why-is-node-still-running": "^1.0.0",
    "ws": "^8.16.0"
  }
}
