{
  "name": "testeranto.ts",
  "description": "teeny tiny tightly-typed typescript tests",
  "version": "0.6.1",
  "main": "index.js",
  "repository": "git@github.com:adamwong246/testeranto.ts.git",
  "homepage": "https://github.com/adamwong246/testeranto.ts",
  "author": "adam wong <adamwong246@gmail.com>",
  "license": "MIT",
  "keywords": [
    "typescript",
    "test",
    "cucumber",
    "ATDD"
  ],
  "scripts": {
    "testeranto!": "ts-node ./tests/index.ts",
    "testrun": "node --inspect ./dist/test.bundle.js",
    "prepublish": "tsc",
    "build": "nodemon --config nodemon.json ./src/build.ts",
    "watch": "ts-node ./src/watch.ts",
    "bundle:reporter": "esbuild ./src/Report.tsx --watch --bundle --outfile=./dist/reporter.js",
    "simpleserver": "cd dist && python -m SimpleHTTPServer 8080",
    "clean:dist": "rm -rf ./dist/results && rm -rf ./dist/tests && rm -f ./dist/reporter.css && rm -f ./dist/reporter.js",
    "clean:npm": "rm -rf node_modules && yarn install",
    "clean": "yarn clean:dist && yarn clean:npm"
  },
  "types": "index.d.ts",
  "dependencies": {
    "@truffle/contract": "^4.6.10",
    "@types/web3": "^1.2.2",
    "bootstrap": "^5.2.3",
    "cancelable-promise": "^4.3.1",
    "eslint": "^8.29.0",
    "ethers": "^5.7.2",
    "fresh-require": "^1.0.3",
    "ganache": "^7.7.2",
    "graphology": "^0.25.1",
    "graphology-dag": "^0.2.0",
    "graphology-types": "^0.24.5",
    "lodash": "^4.17.21",
    "react-bootstrap": "^2.7.0",
    "truffle": "^5.7.1",
    "truffle-compile": "^4.2.3",
    "web3": "^1.8.1",
    "web3-eth-contract": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@reduxjs/toolkit": "^1.8.6",
    "@testing-library/react": "^13.4.0",
    "@types/chai": "^4.3.4",
    "@types/lodash": "^4.14.189",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-loader": "^8.2.5",
    "chai": "^4.3.7",
    "esbuild": "^0.16.5",
    "eslint-plugin-react": "^7.31.11",
    "ignore-loader": "^0.1.2",
    "nodemon": "^2.0.20",
    "puppeteer": "^19.3.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-test-renderer": "^18.2.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
