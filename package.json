{
  "name": "testeranto",
  "description": "teeny tiny tightly-typed typescript tests",
  "version": "0.38.1",
  "type": "module",
  "types": "./dist/index.d.mts",
  "main": "./src/index.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./src/Project": "./dist/Project.js",
    "./src/Features": "./dist/Features.js",
    "./src/Report": "./dist/Report.tsx"
  },
  "repository": "git@github.com:adamwong246/testeranto.git",
  "homepage": "https://github.com/adamwong246/testeranto",
  "author": "adam wong <adamwong246@gmail.com>",
  "license": "MIT",
  "keywords": [
    "typescript",
    "test",
    "cucumber",
    "ATDD"
  ],
  "bin": {
    "testerantoRun": "./bin/testerantoRun.sh",
    "testerantoWatch": "./bin/testerantoWatch.sh"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "bundle:reporter": "esbuild ./src/Report.tsx --bundle --outfile=./dist/reporter.js",
    "simpleserver": "cd dist && python -m SimpleHTTPServer 8080",
    "clean:dist": "rm -rf ./dist/results && rm -rf ./dist/tests  && rm -f ./dist/reporter.css && rm -f ./dist/reporter.js && rm -f ./dist/*.jpg ",
    "clean:npm": "rm -rf node_modules && yarn install",
    "clean": "yarn clean:dist && yarn clean:npm",
    "tableflip": "yarn clean && yarn bundle:reporter"
  },
  "peerDependencies": {
    "esbuild": "0.17.5"
  },
  "devDependencies": {
    "esbuild": "0.17.5"
  },
  "dependencies": {
    "@swc/core": "^1.3.26",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.21",
    "@types/react-bootstrap": "^0.32.32",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-loader": "^8.2.5",
    "bootstrap": "^5.2.3",
    "eslint": "^8.29.0",
    "eslint-plugin-react": "^7.31.11",
    "fresh-require": "^1.0.3",
    "fs.promises.exists": "^1.1.4",
    "graphology": "^0.25.1",
    "graphology-dag": "^0.2.0",
    "graphology-types": "^0.24.5",
    "pm2": "^5.2.2",
    "react-bootstrap": "^2.7.0",
    "ts-node": "^10.9.1",
    "typescript": "4.8.2",
    "why-is-node-running": "^2.2.2",
    "why-is-node-still-running": "^1.0.0"
  }
}