<!--
  This file defines the SMIL 3.0 Document Model.
  All attributes and content models are defined in the second
  half of this file.  We first start with some utility definitions.
  These are mainly used to simplify the use of Modules in the
  second part of the file.
  
  Note that in this model, the Metainformation module is required.
-->
<!-- ================== Util: Body - Content Control ====================== -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" xmlns:its="http://www.w3.org/2005/11/its">
  <xs:import namespace="http://www.w3.org/2005/11/its" schemaLocation="its.xsd"/>
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
  <xs:element name="SMIL.switch-control" abstract="true" substitutionGroup="SMIL.content-control">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="SMIL.switch.content">
          <xs:attributeGroup ref="SMIL.switch.attrib"/>
          <xs:attributeGroup ref="SMIL.Core.attrib"/>
          <xs:attributeGroup ref="SMIL.I18n.attrib"/>
          <xs:attribute name="allowReorder" default="no">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="yes"/>
                <xs:enumeration value="no"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="SMIL.prefetch-control" abstract="true" substitutionGroup="SMIL.content-control">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="SMIL.prefetch.content">
          <xs:attributeGroup ref="SMIL.prefetch.attrib"/>
          <xs:attributeGroup ref="SMIL.Core.attrib"/>
          <xs:attributeGroup ref="SMIL.I18n.attrib"/>
          <xs:attribute name="bandwidth" default="100%"/>
          <xs:attribute name="mediaSize"/>
          <xs:attribute name="mediaTime"/>
          <xs:attribute name="src" type="URI.datatype"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="SMIL.content-control" abstract="true"/>
  <xs:attributeGroup name="SMIL.content-control-attrs">
    <xs:attributeGroup ref="SMIL.Test.attrib"/>
    <xs:attributeGroup ref="SMIL.customTestAttr.attrib"/>
    <xs:attributeGroup ref="SMIL.skip-content.attrib"/>
  </xs:attributeGroup>
  <!-- ================== Util: Head ======================================== -->
  <xs:element name="SMIL.head-meta.content" abstract="true">
    <xs:complexType>
      <xs:attributeGroup ref="SMIL.metadata.attrib"/>
      <xs:attributeGroup ref="SMIL.Core.attrib"/>
      <xs:attributeGroup ref="SMIL.I18n.attrib"/>
    </xs:complexType>
  </xs:element>
  <xs:group name="SMIL.head-textStyling.content">
    <xs:sequence>
      <xs:sequence minOccurs="0">
        <xs:element ref="textStyling"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
      </xs:sequence>
    </xs:sequence>
  </xs:group>
  <xs:group name="SMIL.head-layout.content">
    <xs:sequence>
      <xs:sequence minOccurs="0">
        <xs:choice>
          <xs:element ref="layout"/>
          <xs:element ref="SMIL.switch-control"/>
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
      </xs:sequence>
    </xs:sequence>
  </xs:group>
  <xs:group name="SMIL.head-control.content">
    <xs:sequence>
      <xs:sequence minOccurs="0">
        <xs:element ref="customAttributes"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
      </xs:sequence>
    </xs:sequence>
  </xs:group>
  <xs:group name="SMIL.head-transition.content">
    <xs:sequence>
      <xs:sequence minOccurs="0">
        <xs:element maxOccurs="unbounded" ref="transition"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
      </xs:sequence>
    </xs:sequence>
  </xs:group>
  <xs:group name="SMIL.head-media.content">
    <xs:sequence>
      <xs:sequence minOccurs="0">
        <xs:element maxOccurs="unbounded" ref="paramGroup"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
      </xs:sequence>
    </xs:sequence>
  </xs:group>
  <xs:group name="SMIL.head-state.content">
    <xs:sequence>
      <xs:sequence minOccurs="0">
        <xs:element ref="state"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
      </xs:sequence>
    </xs:sequence>
  </xs:group>
  <xs:group name="SMIL.head-submission.content">
    <xs:sequence>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="submission"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="meta"/>
      </xs:sequence>
    </xs:sequence>
  </xs:group>
  <!-- =================== Util: Body - Animation ============================ -->
  <xs:group name="SMIL.animation.elements">
    <xs:choice>
      <xs:element ref="animate"/>
      <xs:element ref="set"/>
      <xs:element ref="animateMotion"/>
      <xs:element ref="animateColor"/>
    </xs:choice>
  </xs:group>
  <xs:group name="SMIL.simple-animation.elements">
    <xs:choice>
      <xs:element ref="animate"/>
      <xs:element ref="set"/>
    </xs:choice>
  </xs:group>
